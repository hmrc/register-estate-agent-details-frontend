# microservice specific routes

GET        /                                            controllers.IndexController.onPageLoad

GET        /assets/*file                                controllers.Assets.versioned(path="/public", file: Asset)

GET        /language/:lang                              controllers.LanguageSwitchController.switchToLanguage(lang: String)

GET        /this-service-has-been-reset                 controllers.SessionExpiredController.onPageLoad

GET        /check-your-answers                          controllers.CheckYourAnswersController.onPageLoad

GET        /unauthorised                                controllers.UnauthorisedController.onPageLoad

GET        /logout                                      controllers.LogoutController.logout()

GET        /client-reference                              controllers.AgentInternalReferenceController.onPageLoad(mode: Mode = NormalMode)
POST       /client-reference                              controllers.AgentInternalReferenceController.onSubmit(mode: Mode = NormalMode)
GET        /change-client-reference                       controllers.AgentInternalReferenceController.onPageLoad(mode: Mode = CheckMode)
POST       /change-client-reference                       controllers.AgentInternalReferenceController.onSubmit(mode: Mode = CheckMode)

GET        /agent-telephone-number                        controllers.AgentTelephoneNumberController.onPageLoad(mode: Mode = NormalMode)
POST       /agent-telephone-number                        controllers.AgentTelephoneNumberController.onSubmit(mode: Mode = NormalMode)
GET        /change-agent-telephone-number                 controllers.AgentTelephoneNumberController.onPageLoad(mode: Mode = CheckMode)
POST       /change-agent-telephone-number                 controllers.AgentTelephoneNumberController.onSubmit(mode: Mode = CheckMode)

GET        /agentInternationalAddress                        controllers.AgentInternationalAddressController.onPageLoad(mode: Mode = NormalMode)
POST       /agentInternationalAddress                        controllers.AgentInternationalAddressController.onSubmit(mode: Mode = NormalMode)
GET        /changeAgentInternationalAddress                  controllers.AgentInternationalAddressController.onPageLoad(mode: Mode = CheckMode)
POST       /changeAgentInternationalAddress                  controllers.AgentInternationalAddressController.onSubmit(mode: Mode = CheckMode)

GET        /agentName                        controllers.AgentNameController.onPageLoad(mode: Mode = NormalMode)
POST       /agentName                        controllers.AgentNameController.onSubmit(mode: Mode = NormalMode)
GET        /changeAgentName                  controllers.AgentNameController.onPageLoad(mode: Mode = CheckMode)
POST       /changeAgentName                  controllers.AgentNameController.onSubmit(mode: Mode = CheckMode)

GET        /agentUKAddress                        controllers.AgentUKAddressController.onPageLoad(mode: Mode = NormalMode)
POST       /agentUKAddress                        controllers.AgentUKAddressController.onSubmit(mode: Mode = NormalMode)
GET        /changeAgentUKAddress                  controllers.AgentUKAddressController.onPageLoad(mode: Mode = CheckMode)
POST       /changeAgentUKAddress                  controllers.AgentUKAddressController.onSubmit(mode: Mode = CheckMode)
